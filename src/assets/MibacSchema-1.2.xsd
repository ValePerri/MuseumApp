				<element name="tipologia" type="tns:tipoDiLuogo" minOccurs="0" maxOccurs="1"/>
				<element name="descrizione" type="tns:testo" minOccurs="0" maxOccurs="1"/>
				<element name="info" type="tns:info" minOccurs="0" maxOccurs="1"/>
				<element name="indirizzo" type="tns:localizzazione" minOccurs="0" maxOccurs="1"/>
			</sequence>
			<attribute name="ruolo" type="tns:ruoloLuogo" use="required"/>
